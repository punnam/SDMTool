<?xml version="1.0"?> <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dmtool.domain">
	<class name="AdmConfig" table="adm_config">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="envName" column="ENV_NAME" type="string" />
		<property name="admConfigType" column="Config_Type" type="string" />
		<property name="seibelServer" column="SEIBEL_SERVER" type="string" />
		<property name="sessionId" column="SESSION_ID" type="string" />
		<property name="createdTime" column="CREATED_TIME" type="timestamp" />
		<property name="updatedTime" column="UPDATED_TIME" type="timestamp" />
		<property name="createdUser" column="CREATED_USER" type="java.lang.Integer" />
		<property name="updatedUser" column="UPDATED_USER" type="java.lang.Integer" />
	</class>
</hibernate-mapping>