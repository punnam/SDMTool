<?xml version="1.0"?> <!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dmtool.domain">
	<class name="DeploymentOptions" table="deployment_options">
		<id name="id" column="ID" type="java.lang.Integer">
            <generator class="increment"></generator>
        </id>
		<property name="code" column="CODE" type="string" />
		<property name="description" column="DESCRIPTION" type="string" />
		<property name="category" column="category" type="string" />
		<property name="command" column="command" type="string" />
		
	</class>
</hibernate-mapping>