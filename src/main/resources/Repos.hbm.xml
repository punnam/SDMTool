<?xml version="1.0"?> 
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dmtool.domain">
	<class name="Repos" table="export_repos">
		<id name="id" column="ID" type="java.lang.Integer">
			<generator class="increment"></generator>
		</id>
		<property name="envName" column="ENV_NAME" type="string" />
		<property name="userId" column="USER_ID" type="string" />
		<property name="password" column="PASSWORD" type="string" />
		<property name="odbc" column="ODBC" type="string" />
		<property name="filePath" column="FILE_PATH" type="string" />
		<property name="repoType" column="REPO_TYPE" type="string" />
		<property name="repoName" column="REPO_NAME" type="string" />
		<property name="logFilePath" column="LOG_FILE_PATH" type="string" />
		<property name="tableDDLSync" column="TableDDLSync" type="string" />
		<property name="indexDDLSync" column="IndexDDLSync" type="string" />
		<property name="createdTime" column="CREATED_TIME" type="timestamp" />
		<property name="updatedTime" column="UPDATED_TIME" type="timestamp" />
		<property name="createdUser" column="CREATED_USER" type="java.lang.Integer" />
		<property name="updatedUser" column="UPDATED_USER" type="java.lang.Integer" />		
		
	</class>
</hibernate-mapping>